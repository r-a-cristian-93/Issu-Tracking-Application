<web-app>
	<welcome-file-list>
		<welcome-file>/login.html</welcome-file>
	</welcome-file-list>
	<context-param>
		<param-name>sqlAddress</param-name>
		<param-value>jdbc:mysql://hd_database:3306/helpdesk</param-value>
	</context-param>
	<context-param>
		<param-name>sqlUser</param-name>
		<param-value>root</param-value>
	</context-param>
	<context-param>
		<param-name>sqlPassword</param-name>
		<param-value>root</param-value>
	</context-param>
	<context-param>
		<param-name>jwtIssuer</param-name>
		<param-value>helpdesk.com</param-value>
	</context-param>
	<context-param>
		<param-name>jwtKey</param-name>
		<param-value>secret</param-value>
	</context-param>
</web-app>
